
generator client {
  provider = "prisma-client-js"
  output   = "../../node_modules/@prisma/client-mongo"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL_MONGO")
}

model Log {
  id      String   @id @default(auto()) @map("_id") @db.ObjectId
  message String
  fecha   DateTime @default(now())
}